<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.sojuton.skeleton.model.dao.PaperDAO">

    <insert id="insertPaper">
        INSERT INTO tb_paper
        (
            title,
            create_user_seq,
            create_date
        )
        VALUES
        (
            ${title}$,
            ${createUserSeq}$,
            now()
        )
    </insert>
    <select id="" resultType="String">
        SELECT 'test'
    </select>

</mapper>